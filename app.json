      },
      "edgeToEdgeEnabled": true,
      "package": "com.cbelmer28.mibarrio"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a441c0e1-8ed7-4675-95c5-71209314e12f"
      }
    },
    "owner": "cbelmer28",
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Tu app necesita acceder a tu ubicación para mostrar tu posición en el mapa y seguir la ruta de entrega."
        }
      ],
      "expo-notifications"
    ]
